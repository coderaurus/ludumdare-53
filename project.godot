[application]

config/name="Ld- 53"
run/main_scene="res://scenes/Main.tscn"
config/icon="res://icon.png"

[autoload]

MusicManager="*res://scenes/audio/MusicManager.tscn"
SoundManager="*res://scenes/audio/SoundManager.tscn"
